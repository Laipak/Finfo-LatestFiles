            function initMap() {

                // Create a map object and specify the DOM element for display.

                var map1 = new google.maps.Map(document.getElementById('address-1-map'), {

                    center: {lat: 1.282289, lng: 103.850784},

                    scrollwheel: false,

                    zoom: 18

                });



                var marker1 = new google.maps.Marker({

                      position: {lat: 1.282289, lng: 103.850784},

                      map: map1

                });



                var map2 = new google.maps.Map(document.getElementById('address-2-map'), {

                    center: {lat: -37.851468, lng: 144.980215},

                    scrollwheel: false,

                    zoom: 18

                });



                var marker2 = new google.maps.Marker({

                      position: {lat: -37.851468, lng: 144.980215},

                      map: map2

                });

            }

        </script>

